<!-- View people being pursued by multiple users page -->
<h1>Individuals Being Pursued by Multiple Users</h1>
<table border="3" cellpadding="10">
	<tr>
		<th>Person Name</th>
		<th>Number of Users Pursuing</th>
	</tr>
	<% @persongroup.each do |s| %>
	<tr>
		<td><%= s.name %> </td>
		<td><%= s.n_users %> </td>
	</tr>
	<% end %>
</table>
<!-- Button to go back to the people main page -->
<%= button_to "Back to People", people_path , :method => :get %>
