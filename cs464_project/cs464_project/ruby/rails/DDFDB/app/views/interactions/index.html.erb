<!-- Paginated information from the interactions database, generated by rails -->
<%= will_paginate @interactions, :class => 'flickr_pagination' %>
<ul class="users">  <%= render @interactions %>  </ul>
<div class="container">
	<div class="row" style="float:center;">
		<div class="span4">
			<!-- Add new interaction button -->
				<div class="btn-group">
					<a href='/interactions/new'>
						<button class="btn btn-info">Add New Interaction</button>
						<!-- Button to the distinct query -->
						<a href = '/interactions/distinct'>
							<button class="btn btn-success">View locations of dates in the past month</button>
						</div>
					</div>
				</div> <!-- span4 -->
			</div> <!-- row -->
		</div> <!-- container -->


