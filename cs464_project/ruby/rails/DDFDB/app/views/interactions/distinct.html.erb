<!-- Popular places for users to go on dates -->
<h1>Places Users Have Been On Dates Within the Past Month</h1>
<!-- Table generated by rails with locations-->
<table border="3" cellpadding="10">
	<tr>
		<th>Location</th>
	</tr>
	<% @interactiondistinct.each do |s| %>
	<tr>
		<td><%= s.location %> </td>
	</tr>
	<% end %>
</table>
<!-- Button to go back to the interactions main page -->
    <div style="float:left;">
      <%= button_to "Back to Interactions", interactions_path, :method => :get %>
    </div>
</div>
